           <div id="main-content" class="main-content container-fluid">
            <!-- // page head -->

            <div id="page-content" class="page-content">
              <section>
                <div class="row-fluid margin-top20">
                  <div class="span12">
                    <div class="widget widget-box widget-table">
                      <div class="widget-header">
                        <h4><i class="fontello-icon-users"></i> Listagem</h4>
                      </div>
                      <table class="table table-striped table-condensed table-content boo-table">
                        <tr>
                          <th>Cliente</th>
                          <th>Início</th>
                          <th>Término</th>
                          <th>Criado em</th>
                          <th>Valor</th>
                          <th></th>
                          <th></th>
                        </tr>

                        <% @reservas.each do |reserva| %>
                        <tr>
                          <td><%= reserva.cliente.nome %></td>
                          <td><%= reserva.inicio_reserva.strftime("%d/%m/%Y") %></td>
                          <td><%= reserva.termino_reserva.strftime("%d/%m/%Y") %></td>
                          <td><%= reserva.created_at.strftime("%d/%m/%Y - %H:%M:%S") %></td>
                          <td><%= humanized_money_with_symbol reserva.valor %></td>
                          <td><%= link_to 'Show', reserva, class: "btn btn-green btn-mini" %>
                            <%= link_to 'Edit', edit_reserva_path(reserva), class: "btn btn-blue btn-mini" %>
                            <%= link_to 'Destroy', reserva, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-yellow btn-mini" %></td>
                          </tr>
                          <% end %>
                        </table>
                        <div class="widget-footer">
                          <div class="btn-toolbar pull-left">
                            <%= link_to 'Nova Reserva', new_reserva_path, class: "btn btn-blue" %>
                          </div>
                        </div>

                      </div>                           
                    </div>
                  </div>
                </section>
              </div>
