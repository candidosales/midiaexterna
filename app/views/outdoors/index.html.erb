           <div id="main-content" class="main-content container-fluid">
            <!-- // page head -->

            <div id="page-content" class="page-content">
                <section>
                    <div class="row-fluid margin-top20">
                        <div class="span12">
                            <div class="widget widget-box widget-table">
                                <div class="widget-header">
                                    <h4><i class="fontello-icon-users"></i> Listagem</h4>
                                </div>
                                <table class="table table-striped table-condensed table-content boo-table">
                                  <tr>
                                    <th>Número</th>
                                    <th>Endereço</th>
                                    <th>Bairro</th>
                                    <th>Cidade</th>
                                    <th>Valor</th>
                                    <th>Sentido</th>
                                    <th></th>
                                </tr>

                                <% @outdoors.each do |outdoor| %>
                                <tr>
                                    <td><%= outdoor.numero %></td>
                                    <td><%= outdoor.endereco %></td>
                                    <td><%= outdoor.bairro %></td>
                                    <td><%= outdoor.cidade %></td>
                                    <td><%= humanized_money_with_symbol outdoor.valor %></td>
                                    <td><%= outdoor.sentido %></td>
                                    <td><%= link_to 'Show', outdoor, class: "btn btn-green btn-mini" %>
                                        <%= link_to 'Edit', edit_outdoor_path(outdoor), class: "btn btn-blue btn-mini" %>
                                        <%= link_to 'Destroy', outdoor, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-yellow btn-mini" %></td>
                                    </tr>
                                    <% end %>
                                </table>
                                <div class="widget-footer">
                                    <div class="btn-toolbar pull-left">
                                        <%= link_to 'Novo Outdoor', new_outdoor_path, class: "btn btn-blue" %>
                                    </div>
                                </div>

                            </div>                           
                        </div>
                    </div>
                </section>
            </div>
