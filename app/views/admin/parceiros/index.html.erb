                           <div class="well well-nice well-box no-border">
                            <div class="navbar">
                                <div class="navbar-inner">
                                    <h4 class="title"><i class="fontello-icon-users"></i> Listagem</h4>
                                    <ul class="btn-toolbar pull-right">
                                        <li class="divider-vertical"></li>
                                        <li><%= link_to 'Novo Parceiro', new_admin_parceiro_path, class: "btn btn-green" %></li>
                                    </ul>
                                    <!-- // btn-toolbar -->
                                    <ul class="nav nav-pills pull-right">
                                        <li class="active"><a data-toggle="tab" href="#tab1A">PDF</a></li>
                                    </ul>
                                    <!-- // nav-pills --> 
                                </div>
                            </div>
                            <table class="table boo-table table-striped table-content table-hover">
                                <thead>
                                  <tr>
                                    <th class="sorting" scope="col">Nome</th>
                                    <th class="sorting" scope="col">Telefone</th>
                                    <th class="sorting" scope="col">Email</th>
                                    <th class="sorting" scope="col">CNPJ</th>
                                    <th class="sorting" scope="col">Endereço</th>
                                    <th class="sorting" scope="col">Cidade / Estado</th>
                                </tr>

                                <% @parceiros.each do |parceiro| %>
                                <tr>
                                    <td><div class="media-thumb media-left"> 
                <a class="img-shadow" href="javascript:void(0);">
                    <%= image_tag(parceiro.logo.url(:small),:class => "thumb") %>
                </a>
            </div> <%= link_to parceiro.nome, [:admin, parceiro], :class => 'text-left' %>
                                        <span class="action">
                                        <%= link_to '', edit_admin_parceiro_path(parceiro), class: "fontello-icon-edit", title: "Editar" %>
                                        <%= link_to '', [:admin, parceiro], method: :delete, data: { confirm: 'Você tem certeza?' }, class: "fontello-icon-trash", title: "Excluir" %>
                                        </span>
                                        </td>
                                        
                                        <td><%= parceiro.tel %></td>
                                        <td><%= parceiro.email %></td>
                                        <td><%= parceiro.cnpj %></td>
                                        <td><%= parceiro.endereco %>, <%= parceiro.bairro %></td>
                                        <td><%= parceiro.cidade %> / <%= parceiro.estado %></td>
                                    </tr>
                                    <% end %>
                                </table>

                                <div class="action-table">
                                    <div class="btn-toolbar pull-left" style="display:none">
                                        <div class="action-select-wraper">
                                           <span class="arrow-table-action"></span>
                                           <select name="actionTable" id="actionTable" class="selectpicker-boo " >
                                            <option value="" selected="selected">Action for selected...</option>
                                            <option value="validate">Validate</option>
                                            <option value="delete">Delete</option>
                                        </select>
                                        <button type="submit" class="btn btn-boo">Perform the action</button>
                                    </div>
                                </div>
                                <ul class="btn-toolbar pull-right">
                                    <li class="divider-vertical small"></li>
                                    <li><%= link_to 'Novo Parceiro', new_admin_parceiro_path, class: "btn btn-green" %></li>
                                </ul>
                            </div>

                        </div>   
