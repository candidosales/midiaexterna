           <div id="main-content" class="main-content container-fluid">
            <!-- // page head -->

            <div id="page-content" class="page-content">
                <section>
                    <div class="row-fluid margin-top20">
                        <div class="span12">
                            <div class="widget widget-box widget-table">
                                <div class="widget-header">
                                    <h4><i class="fontello-icon-users"></i> Listagem</h4>
                                </div>
                                <table class="table table-striped table-condensed table-content boo-table">
                                    <thead>
                                      <tr>
                                        <th scope="col">Nome</th>
                                        <th scope="col">Email</th>
                                        <th scope="col">Tel</th>
                                        <th scope="col">Cpf</th>
                                        <th scope="col">Cnpj</th>
                                        <th scope="col">Empresa</th>
                                        <th scope="col">Cidade</th>
                                        <th scope="col">Razao</th>
                                        <th scope="col"></th>
                                    </tr>
                                </thead>

                                <% @clientes.each do |cliente| %>
                                <tr>
                                    <td><%= link_to cliente.nome, [:admin, cliente] %></td>
                                    <td><%= cliente.email %></td>
                                    <td><%= cliente.tel %></td>
                                    <td><%= cliente.cpf %></td>
                                    <td><%= cliente.cnpj %></td>
                                    <td><%= cliente.empresa %></td>
                                    <td><%= cliente.cidade %></td>
                                    <td><%= cliente.razao %></td>
                                    <td>
                                    <%= link_to 'Edit', edit_admin_cliente_path(cliente), class: "btn btn-blue btn-mini" %>
                                    <%= link_to 'Destroy', [:admin, cliente], method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-yellow btn-mini" %></td>
                                </tr>
                                <% end %>
                            </table>
                            <div class="widget-footer">
                                <div class="btn-toolbar pull-left">
                                    <%= link_to 'Novo Cliente', new_admin_cliente_path, class: "btn btn-blue" %>
                                </div>
                            </div>

                        </div>                           
                    </div>
                </div>
        </section>
    </div>
